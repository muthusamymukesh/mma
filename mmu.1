#include<stdio.h>
void main()
{
int A[20];
int j,n,k,sum=0;
printf("Enter size of array");
scanf("%d",&n);
printf("Enter k values to sum");
scanf("%d",&k);
printf("Enter elements of array",n);
for(j=0;j<n;j++)
{
scanf("%d",&A[j]);
}
for(j=0;j<k;j++)
{
sum=sum+A[j];
}
printf("Sum of k values are %d",sum);
}
